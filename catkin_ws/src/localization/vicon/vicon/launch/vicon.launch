<launch>
  <node pkg="vicon"
    type="vicon"
    name="vicon"
    output="screen">
    <param name="vicon_server" value="10.52.0.20"/>
    <param name="calib_files_dir" value="$(find vicon)/calib"/>
    <param name="enable_unlabeled_markers" value="false"/>
    <remap from="~rc_car1" to="/vicon/rc_car"/>
    <remap from="~rc_car2" to="/vicon/rc_car"/>
  </node>
</launch>
